import java.util.*;
public class Main {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        String s=sc.nextLine();
        int len=s.length();
        char[] arr=s.toCharArray();

        for(int i=0;i<len;i++){
            if(arr[i]>='A' && arr[i]<='Z'){
                System.out.println();
            }
            System.out.print(arr[i]);
        }

    }
}
